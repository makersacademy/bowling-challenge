{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jasmine": true,
        "jquery": true
    },
    "extends": [
        "standard",
        "plugin:jquery/deprecated",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "semi": [2, "always"]
    }, 
    "plugins": [
        "jasmine",
        "jquery"
    ]
}
