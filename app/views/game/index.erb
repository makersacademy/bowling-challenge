        <td><input type="button" name="7" value="7" /></td>
        <td><input type="button" name="8" value="8" /></td>
        <td><input type="button" name="9" value="9" /></td>
        <td><input type="button" name="10" value="X" /></td>
        <td><input type="button" name="xxx" id="var1-9" value="/" /></td>
      </tr>
      <tr>
        <td colspan=10><input type="button" name="b-new" value="New game" /></td>
      </tr>
    </table>
  </div>
